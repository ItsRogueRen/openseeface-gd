func _ready():
	var skeleton: VSS2DSkeleton = load("res://entities/vss-2d/VSS2DSkeleton.tscn").instance()
	skeleton.name = "Skeleton"
	
	var bone: VSS2DBone = load("res://entities/vss-2d/VSS2DBone.tscn").instance()
	
	skeleton.add_child(bone)
	add_child(skeleton)

